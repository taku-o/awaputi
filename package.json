{
  "name": "bubble-pop-web-game",
  "version": "1.0.0",
  "description": "A web-based bubble popping game with various bubble types and special effects",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:e2e": "playwright test --reporter=list",
    "test:performance": "node tests/performance/performance-test.js",
    "test:i18n-performance": "NODE_OPTIONS='--experimental-vm-modules' jest tests/performance/I18nPerformance.test.js --verbose",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:quality",
    "test:quality": "NODE_OPTIONS='--experimental-vm-modules' jest tests/quality --verbose --runInBand",
    "test:translation-quality": "NODE_OPTIONS='--experimental-vm-modules' jest tests/quality/TranslationQualityTest.js --verbose",
    "test:i18n-usability": "NODE_OPTIONS='--experimental-vm-modules' jest tests/quality/I18nUsabilityTest.js --verbose",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --config=./src/tests/integration/jest.integration.config.js",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:integration:coverage": "npm run test:integration -- --coverage",
    "validate:config": "node scripts/validate-configuration.js",
    "validate:config:verbose": "node scripts/validate-configuration.js --verbose",
    "generate:tests": "node scripts/generate-tests.js",
    "generate:tests:dry-run": "node scripts/generate-tests.js --dry-run",
    "generate:tests:bubble": "node scripts/generate-tests.js --test-type bubble",
    "generate:tests:validate": "node scripts/generate-tests.js --validate-only",
    "prebuild": "npm run validate:config && npm run i18n:setup",
    "i18n:setup": "node scripts/i18n-deployment-setup.js",
    "i18n:validate": "node scripts/i18n-deployment-setup.js --validate-only",
    "i18n:optimize": "node scripts/i18n-deployment-setup.js --optimize-only",
    "i18n:quality-report": "node scripts/generate-translation-quality-report.js",
    "i18n:performance-test": "node scripts/run-i18n-performance-tests.js",
    "validate:i18n-requirements": "node scripts/validate-i18n-requirements.js",
    "prepare:i18n-release": "node scripts/prepare-i18n-release.js",
    "dev": "vite",
    "build": "NODE_ENV=production vite build",
    "build:dev": "NODE_ENV=development vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "build:gh-pages": "NODE_ENV=production VITE_BASE_PATH=/bubble-pop-web-game/ vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "build:force": "NODE_ENV=production vite build --force",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173 --host",
    "clean": "rm -rf dist",
    "deploy:netlify": "node scripts/deploy.js netlify production",
    "deploy:vercel": "node scripts/deploy.js vercel production",
    "deploy:gh-pages": "node scripts/deploy.js gh-pages production",
    "deploy:all": "node scripts/deploy.js all production",
    "deploy:staging": "node scripts/deploy.js netlify staging",
    "optimize": "node scripts/build-optimize.js",
    "lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "size-check": "bundlesize",
    "setup:hooks": "node scripts/setup-git-hooks.js",
    "setup:hooks:force": "node scripts/setup-git-hooks.js --force-default-path",
    "docs:generate": "node tools/api-doc-generator.js",
    "docs:generate:private": "node tools/api-doc-generator.js --include-private",
    "docs:generate:no-examples": "node tools/api-doc-generator.js --no-examples",
    "docs:clean": "rm -rf docs/api-reference",
    "docs:serve": "cd docs/api-reference && python -m http.server 8080",
    "docs:validate": "node tools/doc-validator.js",
    "docs:validate:quick": "node tools/doc-validator.js --no-external-links --no-cross-references",
    "docs:validate:full": "node tools/doc-validator.js --check-all --save-report",
    "docs:check-links": "node tools/link-checker.js",
    "docs:check-links:internal": "node tools/link-checker.js --no-external",
    "docs:check-links:verbose": "node tools/link-checker.js --verbose",
    "docs:watch": "node tools/doc-update-notifier.js",
    "docs:watch:scan": "node tools/doc-update-notifier.js --scan-only",
    "docs:watch:daemon": "node tools/doc-update-notifier.js --daemon",
    "docs:monitor": "node tools/doc-update-notifier.js --interval=60000 --report-interval=1800000",
    "docs:quality": "npm run docs:validate && npm run docs:check-links"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "bundlesize": "^0.18.1",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^11.0.0",
    "terser": "^5.43.1",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "bundlesize": [
    {
      "path": "./dist/assets/*.js",
      "maxSize": "500 kB",
      "compression": "gzip"
    },
    {
      "path": "./dist/assets/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ],
  "keywords": [
    "game",
    "bubble",
    "canvas",
    "web",
    "javascript"
  ],
  "author": "BubblePop Team",
  "license": "MIT"
}
